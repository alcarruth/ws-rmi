{
  "name": "ws_rmi",
  "version": "0.0.1",
  "description": "Remote method invocation over websockets.",
  "main": "build/index.js",
  "scripts": {
    "build": "npm run web && npm run coffee && npm run js6 && npm run js5",
    "web": "npm run html && npm run cat",
    "html": "cp src/client_example.html build/js6",
    "cat": "cat src/ws_rmi_client.coffee src/example_object.coffee src/client_example.coffee > build/coffee/web_client.coffee",
    "coffee": "cp ./src/*.coffee ./build/coffee",
    "js6": "coffee -cMo ./build/js6 ./build/coffee/*.coffee",
    "js5": "npm run js6 && babel ./build/js6 -d ./build/js5",
    "lint": "eslint ./build/js5",
    "clean": "npm run rmdirs && npm run mkdirs",
    "rmdirs": "rm -rf ./build/js5/ ./build/js6/ ./build/coffee/",
    "mkdirs": "mkdir -p ./build/js5/ ./build/js6/ ./build/coffee",
    "test": "mocha --require babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alcarruth/ws_rmi.git"
  },
  "keywords": [
    "npm",
    "node",
    "websocket",
    "remote",
    "object",
    "method",
    "invocation",
    "javascript"
  ],
  "author": "Al Carruth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alcarruth/ws_rmi/issues"
  },
  "homepage": "https://github.com/alcarruth/ws_rmi#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "coffeescript": "^2.3.2",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^5.2.0"
  },
  "dependencies": {
    "coffeescript": "^2.3.2",
    "http": "0.0.0",
    "https": "^1.0.0",
    "ws": "^6.1.3"
  }
}
